   </copyright>

   <license url="http://www.example.org/legal/epl-v10.html">
      EPL 1.0
   </license>

   <url>
      <update label="Moliz Project Updates" url="http://www.modelexecution.org/updates"/>
   </url>

   <includes
         id="org.modelexecution.xmof.feature"
         version="0.0.0"/>

   <includes
         id="org.modelexecution.fumldebug.feature"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore" version="2.9.1" match="greaterOrEqual"/>
      <import plugin="org.modelexecution.fumldebug" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit" version="2.9.0" match="greaterOrEqual"/>
      <import plugin="org.modelexecution.xmof.vm" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.epsilon.ecl.engine" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.epsilon.common"/>
      <import plugin="org.eclipse.epsilon.eol.engine" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.epsilon.emc.emf"/>
      <import plugin="org.eclipse.emf.transaction"/>
      <import plugin="org.modelexecution.xmof.configuration"/>
      <import plugin="org.modelexecution.fuml.convert" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.modelexecution.xmof.states"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.modelexecution.xmof.diff"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
