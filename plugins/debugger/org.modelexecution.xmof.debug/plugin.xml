<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.modelexecution.xmof.debug.launch.XMOFLaunchDelegate"
            id="org.modelexecution.xmof.debug.launchConfigurationType"
            modes="run, debug"
            name="xMOF">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.core.processFactories">
      <processFactory
            class="org.modelexecution.xmof.debug.process.XMOFProcessFactory"
            id="org.modelexecution.xmof.debug.processFactory">
      </processFactory>
   </extension>

</plugin>
